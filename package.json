{
  "name": "flamepass",
  "productName": "Flamepass",
  "version": "1.0.0",
  "description": "Portable web proxy for school environments",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-packager . Flamepass --platform=win32 --arch=x64 --out=dist --overwrite --icon=app/assets/img/logo.ico --asar=false --prune=false",
    "build:linux": "electron-packager . Flamepass --platform=linux --arch=x64 --out=dist --overwrite --icon=app/assets/img/logo.png --asar=false --prune=false"
  },
  "author": "CarbonBoys",
  "license": "MIT",
  "dependencies": {
    "async-exit-hook": "^2.0.1",
    "electron-is-dev": "^2.0.0",
    "lru-cache": "^7.14.1",
    "mime": "^2.5.2",
    "sticky-session-custom": "^1.2.0",
    "testcafe-hammerhead": "31.6.2",
    "uglify-js": "^3.15.3",
    "uuid": "^8.3.2",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv-flow": "^3.2.0",
    "electron": "^29.0.0",
    "electron-packager": "^17.1.1",
    "eslint": "^7.32.0",
    "npm-force-resolutions": "0.0.10",
    "prettier": "^2.3.2"
  },
  "resolutions": {
    "tmp": "0.2.1"
  }
}